{
    "args": ["https://something.rpm"],
    "facts": {
        "file": {
            "_tempfile_.rpm": null
        },
        "rpm_package": {
            "_tempfile_.rpm": null
        }
    },
    "commands": [
        "wget -q https://something.rpm -O _tempfile_.rpm",
        "rpm -q `rpm -qp _tempfile_.rpm` 2> /dev/null || rpm -i _tempfile_.rpm"
    ]
}
